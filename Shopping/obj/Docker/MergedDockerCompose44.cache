name: dockercompose16807307115300097864
services:
  shopping.api:
    build:
      context: C:\Users\김정우\source\repos\test\Shopping
      dockerfile: Shopping.API/Dockerfile
    container_name: shoppingapi
    depends_on:
      shopping.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8000"
    image: shoppingapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8000
        published: "8000"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\김정우\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\김정우\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  shopping.client:
    build:
      context: C:\Users\김정우\source\repos\test\Shopping
      dockerfile: Shopping.Client/Dockerfile
    container_name: shoppingclient
    depends_on:
      shopping.api:
        condition: service_started
        required: true
      shopping.db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORTS: "8001"
    image: shoppingclient
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8001"
        protocol: tcp
  shopping.db:
    container_name: shoppingdb
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: /site
        target: /data/db
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose16807307115300097864_default